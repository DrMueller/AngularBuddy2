{
	"name": "drmueller2-angularbuddy2",
	"displayName": "AngularBuddy2",
	"description": "Angular Buddy",
	"version": "0.0.1",
	"icon": "assets/M.png",
	"publisher": "DrMueller2",
	"homepage": "https://github.com/DrMueller/AngularBuddy2",
	"repository": {
		"type": "git",
		"url": "https://github.com/DrMueller/AngularBuddy2"
	},
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.deleteAllSpecFiles",
				"title": "Delete all spec files"
			}
		],
		"keybindings": {
			"command": "extension.deleteAllSpecFiles",
			"key": "ctrl+shift+a",
			"when": "editorTextFocus"
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"lint": "tslint --project tslint.json",
		"ci-test": "npm run compile && node ./node_modules/vscode/bin/test",
		"ci-lint": "tslint --project tslint.json --format junit --out testresults/lint-tests.xml",
		"ci-git-clean": "git reset --hard",
		"ci-git-config": "git config --global user.email \"matthiasm@live.de\" && git config --global user.name \"Matthias Mueller\"",
		"ci-vsce-publish": "vsce publish",
		"ci-execute-tests": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"inversify": "^5.0.1",
		"reflect-metadata": "^0.1.13",
		"vsce": "^1.66.0"
	}
}